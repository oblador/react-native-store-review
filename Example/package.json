{
  "name": "Example",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "postinstall": "DESTINATION='node_modules/react-native-store-review' LIB_FILE=`cd .. && echo \\`pwd\\`/\\`npm pack\\`` && (rm -rf $DESTINATION || true) && mkdir $DESTINATION && tar -xvzf $LIB_FILE -C $DESTINATION --strip-components 1 && rm $LIB_FILE",
