  "dependencies": {
    "react": "16.13.1",
    "react-native": "0.63.4",
    "react-native-store-review": "file:../"
  },
  "devDependencies": {
    "@babel/core": "^7.12.17",
    "@babel/runtime": "^7.12.18",
    "@react-native-community/eslint-config": "^2.0.0",
    "babel-jest": "^26.6.3",
    "eslint": "^7.20.0",
    "jest": "^26.6.3",
    "metro-react-native-babel-preset": "^0.65.1",
    "react-test-renderer": "16.13.1"
  },
  "jest": {
    "preset": "react-native"
  }
}
